# Tradier API Configuration
# Copy this file to .env and add your credentials

# Sandbox (Paper Trading) - Get token at https://developer.tradier.com/
TRADIER_SANDBOX_TOKEN=your_sandbox_token_here
TRADIER_SANDBOX_URL=https://sandbox.tradier.com/v1

# Production (Live Trading) - Get token from your Tradier account
TRADIER_PROD_TOKEN=your_production_token_here
TRADIER_PROD_URL=https://api.tradier.com/v1

# Environment: sandbox or production
TRADIER_ENV=sandbox

# Account number (optional, will be fetched if not provided)
TRADIER_ACCOUNT_ID=